{
  "name": "ts-react-antd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-alloy": "^4.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "set PORT=5000 && craco start FAST_REFRESH=true",
    "build": "set GENERATE_SOURCEMAP=false && craco build",
    "analyzer": "env NODE_ENV=production BUILD_ANALYZER=true yarn start",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint {src} --ext .ts,.tsx,.js,jsx",
    "lint_fix": "eslint {src} --ext .ts,.tsx,.js,jsx --fix",
    "lint:css": "stylelint src/**/*.less --fix  --custom-syntax postcss-less"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@craco/craco": "^6.4.3",
    "antd": "^4.18.2",
    "babel-plugin-import": "^1.13.3",
    "craco-less": "^2.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "postcss-less": "^5.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-webpack-plugin": "^3.1.0"
  }
}
